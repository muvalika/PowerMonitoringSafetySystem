        },
        {
            "name": "Attach Camel Debugger with JMX",
            "type": "apache.camel",
            "request": "attach",
            "attach_jmx_url": "service:jmx:rmi:///jndi/rmi://localhost:1099/jmxrmi/camel"
        },
        {
            "name": "Attach Camel Debugger",
            "type": "apache.camel",
            "request": "attach"
        },
        {
            "name": "Attach Camel Debugger with PreLaunch",
            "type": "apache.camel",
            "request": "attach",
            "preLaunchTask": "Start Camel application"
        }
    ]
